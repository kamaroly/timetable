{
    "name": "kamaro/timetable",
    "type": "library",
    "description": "A package to generate weekly timetable",
    "keywords": [
        "Kamaro",
        "TimeTable"
    ],
    "homepage": "https://github.com/kamaroly/timetable",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamaro",
            "email": "kamaroly@gmail.com",
            "homepage": "https://github.com/kamaroly",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "nesbot/carbon": "^2.20"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kamaro\\TimeTable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamaro\\TimeTable\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
